{
  "id": "cereal:mod-loading-screen",
  "script-path": "cereal:scripts/ui/mod-loading-screen.js",
  "meta": {
    "styles": ["cereal:ui/styles/mod-loading-progress-bar.json"],
    "background-color": "black"
  },
  "elements": [
    {
      "type": "cereal:pulsing-image",
      "id": "puzzle-logo",
      "meta": {
        "image": "cereal:textures/puzzle-loader-icon.png",
        "frequency": 0.2,
        "max-scale": 1.1,
        "min-scale": 1
      },
      "anchors": ["right", "bottom"],
      "pos": [-5, -5],
      "size": [64, 64]
    },
    {
      "type": "cereal:progress-bar",
      "id": "ram-usage-bar",
      "meta": {
        "style": {
          "foreground-full": "#ababab",
          "foreground-empty": "#3d3d3d",
          "background": "#ffffff"
        }
      },
      "anchors": ["center", "top"],
      "pos": [0, 5],
      "size": [500, 21]
    },
    {
      "type": "cereal:progress-bar",
      "id": "progress-bar-1",
      "meta": {
        "style": "cereal:mod-loading-progress-bar"
      },
      "anchors": ["center", "none"],
      "pos": [0, 40],
      "size": [500, 30]
    },
    {
      "type": "cereal:progress-bar",
      "id": "progress-bar-2",
      "meta": {
        "style": "cereal:mod-loading-progress-bar"
      },
      "anchors": ["center", "none"],
      "pos": [0, 80],
      "size": [500, 30]
    },
    {
      "type": "cereal:progress-bar",
      "id": "progress-bar-3",
      "meta": {
        "style": "cereal:mod-loading-progress-bar"
      },
      "anchors": ["center", "none"],
      "pos": [0, 120],
      "size": [500, 30]
    },
    {
      "type": "cereal:button",
      "id": "button",
      "meta": {
        "style": {
          "outline-thickness": 5,
          "outline-color": "spicy-skin",
          "background-color": "spicy-eye",
          "outline-hover-color": "crab-skin",
          "background-hover-color": "crab-eye",
          "outline-press-color": "zombii-skin",
          "background-press-color": "zombii-eye"
        }
      },
      "anchors": ["center", "top"],
      "pos": [0, 60]
    }
  ]
}