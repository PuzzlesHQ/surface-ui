{
  "id": "cereal:mod-loading-screen",
  "script-path": "cereal:scripts/ui/mod-loading-screen.js",
  "meta": {
    "styles": ["cereal:ui/styles/mod-loading-progress-bar.json"],
    "background-color": "black"
  },
  "elements": [
    {
      "type": "cereal:progress-bar",
      "id": "ram-usage-bar",
      "meta": {
        "style": {
          "foreground-full": "#ababab",
          "foreground-empty":  "#3d3d3d",
          "background": "#ffffff"
        }
      },
      "anchors": ["center", "top"],
      "pos": [0, 5],
      "size": [500, 21]
    },
    {
      "type": "cereal:progress-bar",
      "id": "progress-bar-1",
      "meta": {
        "style": "cereal:mod-loading-progress-bar"
      },
      "anchors": ["center", "none"],
      "pos": [0, 40],
      "size": [500, 30]
    },
    {
      "type": "cereal:progress-bar",
      "id": "progress-bar-2",
      "meta": {
        "style": "cereal:mod-loading-progress-bar"
      },
      "anchors": ["center", "none"],
      "pos": [0, 80],
      "size": [500, 30]
    },
    {
      "type": "cereal:progress-bar",
      "id": "progress-bar-3",
      "meta": {
        "style": "cereal:mod-loading-progress-bar"
      },
      "anchors": ["center", "none"],
      "pos": [0, 120],
      "size": [500, 30]
    },
    {
      "type": "cereal:button",
      "id": "button",
      "meta": {
        "style": {
          "outline-thickness": 5
        }
      },
      "anchors": ["center", "top"],
      "pos": [0, 60]
    }
  ]
}