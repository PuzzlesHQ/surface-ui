{
  "id": "cereal:mod-loading-screen",
  "script-path": "cereal:scripts/ui/mod-loading-screen.js",
  "meta": {
    "styles": ["cereal:ui/styles/mod-loading-progress-bar.json"]
  },
  "elements": [
    {
      "type": "cereal:progress-bar",
      "id": "ram-usage-bar",
      "meta": {
        "style": {
          "foreground-full": [171, 171, 171],
          "foreground-empty":  [61, 61, 61],
          "background": [255, 255, 255]
        }
      },
      "anchors": ["center", "top"],
      "pos": [0, 5],
      "size": [500, 21]
    },
    {
      "type": "cereal:progress-bar",
      "id": "progress-bar-1",
      "meta": {
        "style": {
          "foreground-full": [7, 99, 10],
          "foreground-empty": [3, 51, 5],
          "background": [20, 212, 26]
        }
      },
      "anchors": ["center", "none"],
      "pos": [0, 40],
      "size": [500, 30]
    },
    {
      "type": "cereal:progress-bar",
      "id": "progress-bar-2",
      "meta": {
        "style": "cereal:mod-loading-progress-bar"
      },
      "anchors": ["center", "none"],
      "pos": [0, 90],
      "size": [500, 30]
    },
    {
      "type": "cereal:progress-bar",
      "id": "progress-bar-3",
      "meta": {
        "style": "cereal:mod-loading-progress-bar"
      },
      "anchors": ["center", "none"],
      "pos": [0, 120],
      "size": [500, 30]
    }
  ]
